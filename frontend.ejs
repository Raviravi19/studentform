<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Database</title>
</head>
<body>
  <% if(studentdetails.length>0){ %>
    <% for(var i=0;i<studentdetails.length;i++){ %>
        <h3>FirstName- <%- studentdetails[i].fname%></h3>
        <h3>LastName- <%- studentdetails[i].lname%></h3>
        <h3>Phone- <%- studentdetails[i].phone%></h3>
        <h3>Email- <%- studentdetails[i].mail%></h3>
        <h3>Degree- <%- studentdetails[i].degree%></h3>
        <h3>Course- <%- studentdetails[i].course%></h3>
        <hr>
        <% }%>
        <%}else{%>
        <h3>student <details> not found</details></h3>
        <%} %>
</body>
</html>
